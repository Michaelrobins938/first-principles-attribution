{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Bootstrap UQ Artifact",
  "description": "Bootstrap resampling results for path sampling uncertainty quantification",
  "type": "object",
  "required": ["ir_version", "n_bootstrap_samples", "confidence_level", "channel_estimates"],
  "properties": {
    "ir_version": { "type": "string", "pattern": "^[0-9]+\\.[0-9]+\\.[0-9]+$" },
    "generated_at": { "type": "string", "format": "date-time" },
    "n_bootstrap_samples": { "type": "integer", "minimum": 100, "maximum": 10000 },
    "confidence_level": { "type": "number", "minimum": 0.8, "maximum": 0.99 },
    "channel_estimates": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "properties": {
          "point_estimate": { "type": "number" },
          "standard_error": { "type": "number" },
          "ci_90": {
            "type": "object",
            "properties": {
              "p05": { "type": "number" },
              "p25": { "type": "number" },
              "p50": { "type": "number" },
              "p75": { "type": "number" },
              "p95": { "type": "number" }
            }
          },
          "bias_corrected_estimate": { "type": "number" },
          "bias_correction": { "type": "number" }
        }
      }
    },
    "global_metrics": {
      "type": "object",
      "properties": {
        "avg_ci_width": { "type": "number" },
        "max_ci_width": { "type": "number" },
        "min_ci_width": { "type": "number" },
        "overall_stability": { "type": "number" }
      }
    }
  }
}